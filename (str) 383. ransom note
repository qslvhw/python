class Solution(object):
    def canConstruct(self, ransomNote, magazine):
        """
        :type ransomNote: str
        :type magazine: str
        :rtype: bool
        """
        Note_cnt = collections.Counter(ransomNote)
        maga_cnt = collections.Counter(magazine)
        return not Note_cnt - maga_cnt
        
        
        
        
        
        
        
        ######   collections.Counter() #######
