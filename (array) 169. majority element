class Solution(object):
    def majorityElement(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        cnt = 0
        temp = None
        
        for n in nums:
            if cnt == 0:
                temp = n
            
            if temp == n:
                cnt += 1
            else:
                cnt -= 1
        return temp
