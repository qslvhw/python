class Solution(object):
    def reverse(self, x):
        """
        :type x: int
        :rtype: int
        """
        import numpy
        ans = 0
        sign = 1 if x > 0 else -1
        x = abs(x)
        while x > 0:
            ans = ans * 10 + x % 10
            x /= 10
        
        if ans > 2147483647L: return 0
        else: return ans * sign
        
