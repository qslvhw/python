class solution:
   def twoSum(self, nums, target):
        if len(nums)==0:
            return -1

        n=len(nums)
        i=0
        j=i+1
        for i in range(0,n):
            for j in range(i+1,n):
                if nums[i]==target-nums[j]:
                    return i,j
                j+=1
            i+=1
s=solution()
print s.twoSum([1,2,3,4,5],5)
